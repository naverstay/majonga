include ../components/profile_photo_menu

.board_holder
  .board_title._profile_controls
    a.title_back_btn.dm.goBackBtn(href='index.html')
      img(src='img/svg/back-left-arr.svg')
    span.fz_22.dm Моя анкета

    +profile_title_controls()

  .board_block._photos_mode
    form(action='#'): .profile_photos_holder
      .profile_photos_list
        .boardGrid_.clearfix
          +loop_while(1)
            .profile_photo_unit.gridItem._wide: .profile_photo_w
              .profile_photo._no_shadow
                .swiper-container.myPhotoSlider.my_photo_slider
                  .swiper-wrapper
                    +loop_while(7)
                      .slide.swiper-slide
                        img(src='img/my_photo.jpg')
              +profile_photo_menu(false, 0, false, false, false, 25, true)
            .profile_photo_unit.gridItem.land_only: .profile_photo_w
              a.profile_photo._no_shadow.galItem(href='img/my_photo_big.jpg')
                img(src='img/my_photo.jpg')
              +profile_photo_menu(false, 0)
            .profile_photo_unit.gridItem.land_only: .profile_photo_w
              .profile_photo._no_shadow._empty

            .profile_photo_unit.gridItem.land_only: .profile_photo_w
              .profile_photo._no_shadow._profile_address
                p: span Россия, Санкт-Петербург,<br>м. Проспект Большевиков<br>Гороскоп: Близнецы
                p: span.clr_gray Хочу найти:<br>Парня или Девушку 18–25 лет<br>из Санкт-Петербурга

        .questionnaire_tip.land_only
          p
            span.fz_16.clr_gray Это ваша анкета. Именно так её видят другие пользователи.
          p
            span.fz_16.clr_gray Вы можете редактировать блоки, а можете их не редактировать.

      .questionnaire_aside
        //-.questionnaire_ratings.land_only
          .questionnaire_rating_col
            p
              span.fz_14 Позиция<br>на сайте
            .rating_counter._total
              span.dm 282

          .questionnaire_rating_col
            p
              span.fz_14 Просмотры<br>за последний месяц
            .rating_counter._month
              span.dm 1 585

        .questionnaire_mob_info.land_hidden
          p
            span.fz_26 Хочу найти: Парня или Девушку 18–25 лет
          p
            span.clr_gray.fz_22 Просмотров за последние 30 дней: 266

        .questionnaire_gifts
          .questionnaire_gift_list: .giftSlider.swiper-container
            .swiper-wrapper
              +loop_while(1)
                .questionnaireGift.questionnaire_slide.swiper-slide
                  .questionnaire_gift
                    a.gift_rm_btn.giftRmBtn(href='#')
                      +menu_icon('close')
                    img(src='img/gift_1.jpg')
                .questionnaireGift.questionnaire_slide.swiper-slide
                  .questionnaire_gift
                    a.gift_rm_btn.giftRmBtn(href='#')
                      +menu_icon('close')
                    img(src='img/gift_2.jpg')
                //-.questionnaireGift.questionnaire_slide.land_hidden.swiper-slide
                  .questionnaire_gift
                    a.gift_rm_btn.giftRmBtn(href='#')
                      +menu_icon('close')
                    img(src='img/gift_3.jpg')

                .questionnaireGift.questionnaire_slide.swiper-slide
                  .questionnaire_gift
                    a.gift_rm_btn.giftRmBtn(href='#')
                      +menu_icon('close')
                    img(src='img/gift_4.jpg')
                .questionnaireGift.questionnaire_slide.swiper-slide
                  .questionnaire_gift
                    a.gift_rm_btn.giftRmBtn(href='#')
                      +menu_icon('close')
                    img(src='img/gift_4.jpg')

          //-.questionnaire_gifts_more
            a.gift_more_btn(href='#')
              span.fz_14 Все

    .profile_settings_mob_collapse.land_hidden
      .fl
        span.fz_40 Анкета
      .fr.profile_settings_control_right.settingsCollapseAll
        span.fz_26 Раскрыть все блоки
